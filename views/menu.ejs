        <div class="navbar-nav">
          <a href="/home">Home</a>
  
          <a href="/aboutUs">About Us</a>
  
          <a href="/menu">Menu</a>
  
          <a href="/contactUs">Contact</a>
        </div>

        <div class="navbar-ext">
